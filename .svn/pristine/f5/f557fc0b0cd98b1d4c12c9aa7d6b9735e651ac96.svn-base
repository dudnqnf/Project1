<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_log_bg"
    android:orientation="vertical"
    android:focusable="true"
    android:focusableInTouchMode="true" >

    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/title_bar"
        android:background="@drawable/title_setting"
        >
   
    </RelativeLayout>

    
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        	<LinearLayout 
        	    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
            	android:layout_gravity="center_horizontal"
			    android:orientation="vertical">
			    
        	    <ImageView
			        android:id="@+id/imageView2"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginTop="34dp"
			        android:layout_gravity="center_horizontal"
			        android:src="@drawable/join_basic_text_01" />

				    
			    <EditText
			        android:id="@+id/basic_male_name_edit"
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/edit_box_height"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="5dp"
			        android:layout_marginLeft="5dp"
			        android:layout_marginRight="5dp"
			        android:background="@drawable/img_edit_box"
			        android:singleLine="true"
			        android:ems="10" >
			
			    </EditText>
			
			    <ImageView
			        android:id="@+id/imageView3"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="25dp"
			        android:src="@drawable/join_basic_text_02" />
			
			    <EditText
			        android:id="@+id/basic_female_name_edit"
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/edit_box_height"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="5dp"
			        android:layout_marginLeft="5dp"
			        android:layout_marginRight="5dp"
			        android:background="@drawable/img_edit_box"
			        android:singleLine="true"
			        android:ems="10" />
			
			    <ImageView
			        android:id="@+id/imageView4"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="25dp"
			        android:src="@drawable/join_basic_text_03" />
			
			    <Button
			        android:id="@+id/basic_date_btn"
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/edit_box_height"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="5dp"
			        android:layout_marginLeft="5dp"
			        android:layout_marginRight="5dp"
			        android:background="@drawable/img_edit_box"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:minHeight="1dip"
			        android:minWidth="1dip" />
			
			    <ImageView
			        android:id="@+id/imageView5"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginTop="25dp"
			        android:src="@drawable/join_basic_text_04"
			        android:layout_gravity="center_horizontal" />
			
			    <Button
			        android:id="@+id/basic_time_btn"
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/edit_box_height"
			        android:layout_gravity="center_horizontal"
			        android:layout_marginTop="5dp"
			        android:layout_marginLeft="5dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginBottom="25dp"
			        android:background="@drawable/img_edit_box"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:minHeight="1dip"
			        android:minWidth="1dip" />

			    
        	    <ImageView
			        android:id="@+id/imageView6"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:src="@drawable/join_basic_text_05"
			        android:layout_gravity="center_horizontal" />
				
        	    
        	    <LinearLayout 
        	        android:layout_width="match_parent"
        	        android:layout_height="@dimen/edit_box_height"
        	        android:layout_marginTop="5dp"
			        android:layout_marginLeft="5dp"
				    android:layout_marginRight="5dp">
				    
				    <EditText
				        android:id="@+id/basic_place_edit"
				        android:layout_width="0dp"
				        android:layout_height="@dimen/edit_box_height"
				        android:layout_gravity="center_horizontal"
				        android:layout_weight="1"
				        android:background="@drawable/img_edit_box"
				        android:singleLine="true"
				        android:ems="10"
				        android:gravity="center_vertical" />
				    
				    <Button 
				        android:id="@+id/naver_map"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_gravity="center_vertical"
				        android:background="@drawable/btn_map_make"
				        android:layout_marginLeft="5dp"/>
			    
			    </LinearLayout>
			    
	    		<Button
			        android:id="@+id/basic_next_btn"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginTop="20dp"
			        android:minHeight="1dp"
			        android:layout_marginBottom="10dp"
			        android:background="@drawable/btn_small_confirm"
			        android:layout_gravity="center_horizontal" />
	    		
        	</LinearLayout>   
	</ScrollView>
</LinearLayout>