<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/detail_like_btn"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img_starting_back" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_horizontal"
        android:orientation="vertical"
        android:layout_marginBottom="70dp">
        
            
            <Button
	            android:id="@+id/start_join_btn"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:background="@drawable/btn_starting_start"
	            android:minHeight="1dp"/>

 
           <Button
	            android:id="@+id/start_enter_btn"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="center_horizontal"
	            android:layout_marginTop="20dp"
	            android:background="@drawable/btn_starting_invite" 
	            android:minHeight="1dp"/>

  
	</LinearLayout>
</FrameLayout>
